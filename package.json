{
  "name": "lucian-pets",
  "version": "0.1.0",
  "description": "AI-Powered Virtual Pet with Lucian Cognitive Architecture & ODIN Sensory Perception",
  "main": "src/main.tsx",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ext ts,tsx",
    "type-check": "tsc --noEmit",
    "server": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "setup": "chmod +x scripts/setup.sh && scripts/setup.sh",
    "check-ports": "node scripts/check-ports.js",
    "start:all": "concurrently \"npm run dev\" \"npm run server\"",
    "lucian:status": "node scripts/check-lucian.js",
    "odin:status": "node scripts/check-odin.js",
    "pet:export": "node scripts/export-pet.js",
    "pet:import": "node scripts/import-pet.js"
  },
  "dependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^9.3.0",
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/three": "^0.158.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "three": "^0.158.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vitest": "^0.34.0"
  },
  "keywords": [
    "ai",
    "virtual-pet",
    "lucian",
    "odin",
    "cognitive-architecture",
    "educational",
    "consciousness",
    "neuroforge",
    "sensory-perception"
  ],
  "author": "Aeryn White",
  "license": "MIT"
}
